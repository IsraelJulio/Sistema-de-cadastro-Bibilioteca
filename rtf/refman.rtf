{\bkmkstart AAAAAAAAEJ}
{\bkmkend AAAAAAAAEJ}
{
\pard\plain \s61\li360\sa60\sb30\qj\widctlpar\qj\adjustright \fs20\cgrid 
\par
{
{\s17\sa60\sb30\widctlpar\qj \fs22\cgrid 
Pegar todos os usuarios. }}\par
{\s17\sa60\sb30\widctlpar\qj \fs22\cgrid 
\par
{{\s5\sb90\sa30\keepn\widctlpar\adjustright \b\f1\fs20\cgrid 
Retorna\par}\pard\plain \s82\li720\widctlpar\ql\adjustright \fs20\cgrid {\s17 \sa60 \sb30
vector<User*> \par
}}}{
\pard\plain \s41\li360\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid 58                                \{\par
59     vector<User*> usuariosSalvos;\par
60     vector<Livro*> livrosSalvos;\par
61     {\cf18 string} conteudo = {\cf22 ""};\par
62     ifstream arq({\cf22 "Usuarios.txt"});\par
63     vector<string> value;\par
64     {\cf19 if} (arq.is_open())\par
65     \{\par
66         {\cf19 while} (getline(arq, conteudo))\{\par
67           Operacoes::split(conteudo.begin(),conteudo.end(),{\cf23 ','}, back_inserter(value));  \par
68           {\cf19 if} (stoi(value[2]) == 1)\par
69           \{ \par
70             Administrador* admin = {\cf17 new} Administrador(value[0],value[1],EPerfil::ADMIN,stoi(value[3])==1);\par
71             usuariosSalvos.push_back(admin);\par
72           \} {\cf19 else} \{\par
73             {\cf19 for} ({\cf18 long} {\cf18 unsigned} {\cf18 int} o = 4; o < value.size(); o++)\par
74             \{\par
75                 {\cf17 auto} lv = Livro::GetById(stoi(value[o]));\par
76                 Livro* livro = {\cf17 new} Livro(lv->GetId(),lv->GetName(), lv->GetGenre(),lv->GetStats());                        \par
77                 livrosSalvos.push_back(livro);                \par
78             \}\par
79             Usuario* user = {\cf17 new} Usuario(value[0],value[1],EPerfil::USER,stoi(value[3])==1);\par
80             user->_meusLivros = livrosSalvos;\par
81             usuariosSalvos.push_back(user);\par
82           \}\par
83           livrosSalvos.clear();\par
84           value.clear();\par
85         \}\par
86         arq.close();\par
87     \}   \par
88     {\cf19 return} usuariosSalvos;\par
89 \}\par
}
}
